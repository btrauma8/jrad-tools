    "rollup-plugin-postcss": "^3.1.8",
    "rollup-plugin-styles": "3.10.0",
    "tsdx": "^0.13.3",
    "tslib": "^2.0.1",
    "typescript": "3.9.7",
    "@types/react-calendar": "3.1.1"
  }
