    "@popperjs/core": "2.5.2",
    "react-popper": "2.2.3"
  },
  "husky": {
    "hooks": {
      "none-pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "tabWidth": 4,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
